namespace ImageIndexer;

struct Pixel
{
    red:int;
    green:int;
    blue:int;
}

table Macroblock
{
    pixels:[Pixel];
}

table ImageFingerPrint
{
    filePath:string;
    macroblocks:[Macroblock];
}

table ImageFingerPrintDatabase
{
    fingerprints:[ImageFingerPrint];
}

root_type ImageFingerPrintDatabase;